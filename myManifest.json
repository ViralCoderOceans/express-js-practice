{
  "endpoints": {
    "/games": {
      "get": {
        "operation": "getAllGames"
      },
      "post": {
        "operation": "addGame"
      }
    },
    "/games/:id": {
      "get": {
        "operation": "getGameById"
      },
      "put": {
        "operation": "updateGame"
      },
      "delete": {
        "operation": "deleteGame"
      }
    }
  }
}